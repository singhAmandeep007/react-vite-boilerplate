{
  "name": "vite-react-boilerplate",
  "author": {
    "name": "Amandeep Singh",
    "email": "amandeep.singh.fsd@gmail.com"
  },
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "preview:test": "npm run build && start-server-and-test preview http://localhost:5001",
    "init:msw": "msw init ./msw --no-save",
    "lint": "eslint --report-unused-disable-directives --max-warnings 5 \"{src,cypress}/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html,md,json}\"",
    "tsc:check": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
    "tsc:watch": "tsc --project ./tsconfig.json --noEmit --skipLibCheck --watch",
    "TEST-START": "--------------------------------------------------------------------------",
    "test": "run-s test:unit",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:watch": "vitest watch",
    "test:unit:typecheck": "vitest run --typecheck",
    "test:unit:ui": "vitest --ui --coverage.enabled=true",
    "test:e2e": "npm run preview:test 'cypress open'",
    "test:e2e:headless": "npm run preview:test 'cypress run'",
    "TEST-END": "----------------------------------------------------------------------------",
    "COMMIT-START": "------------------------------------------------------------------------",
    "commit:pre": "lint-staged",
    "commit:init": "git-cz",
    "commit:devmoji": "devmoji -e --lint",
    "COMMIT-END": "--------------------------------------------------------------------------",
    "prepare": "husky || true",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-router": "^1.81.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "ky": "^1.7.2",
    "lucide-react": "^0.456.0",
    "radash": "^12.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-i18next": "^15.1.1",
    "superjson": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.13.0",
    "@faker-js/faker": "^9.2.0",
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-a11y": "^8.4.3",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-links": "^8.4.3",
    "@storybook/addon-themes": "^8.4.3",
    "@storybook/blocks": "^8.4.3",
    "@storybook/react": "^8.4.3",
    "@storybook/react-vite": "^8.4.3",
    "@storybook/test": "^8.4.3",
    "@tanstack/react-query-devtools": "^5.59.20",
    "@tanstack/router-devtools": "^1.81.5",
    "@tanstack/router-plugin": "^1.79.0",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cypress": "^13.15.2",
    "cz-conventional-changelog": "^3.3.0",
    "devmoji": "^2.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "start-server-and-test": "^2.0.8",
    "storybook": "^8.4.3",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
